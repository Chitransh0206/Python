# def goodDay(name, ending):
#     print("Good day,", name  + "!")
#     print(ending)

# goodDay("Harry", "Thank you")  # function call  


def goodDay(name, ending):
    print("Good day,", name  + "!")
    print(ending)
    return "Done"

a = goodDay("Harry", "Thank you")  # function call 
print(a)  # prints the return value of the function